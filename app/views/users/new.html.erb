<h1>New User</h1>

<%= link_to 'Back', 'index', :user_id => @user.id %>

<%= form_for :user, :url => {:action => 'create', :user_id => @user.id} do |f| %>

    <%= render :partial => "form", :locals => {:f => f} %>

    <div class="form-buttons">
      <%= submit_tag "Create User" %>
    </div>

<% end %>
